{
  "name": "strapi-next-starter",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "frontend",
      "backend"
    ],
    "nohoist": [
      "**/@strapi/**",
      "**/@strapi**",
      "**/sharp",
      "**/@img/**",
      "**/@img**",
      "**/libvips",
      "backend/**"
    ]
  },
  "scripts": {
    "dev:frontend": "npm --workspace frontend run dev",
    "dev:backend": "npm --workspace backend run develop",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
